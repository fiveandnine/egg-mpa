{
  "name": "zhinanzhen_v2",
  "version": "1.0.0",
  "description": "Zhinanzhen toc project",
  "private": true,
  "dependencies": {
    "egg": "^2.2.1",
    "egg-scripts": "^2.5.0",
    "egg-view-nunjucks": "^2.2.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "easywebpack-cli": "^4.3.2",
    "easywebpack-html": "^4.0.0",
    "egg-alinode": "^2.0.1",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "husky": "^1.2.1",
    "imagemin-webpack-plugin": "^2.3.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-zhinanzhen_v2",
    "stop": "egg-scripts stop --title=egg-server-zhinanzhen_v2",
    "egg:dev": "egg-bin dev",
    "webpack:dev": "easy dev",
    "build": "easy build",
    "print": "easy print",
    "dev": "npm-run-all -p webpack:dev egg:dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "commit": "git-cz"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "toc team",
  "license": "MIT"
}
